{
  "Lua.runtime.version": "Lua 5.3",
  "Lua.workspace.checkThirdParty": false,

  // Clearly separated stub libraries
  "Lua.workspace.library": [
    // "${workspaceFolder}/.stubs/snd-stubs.lua",  // Local SND stubs
    "${env:APPDATA}/XIVLauncher/pluginConfigs/SomethingNeedDoing/stubs", // PluginConfig stubs
    "C:/Users/Administrator/AppData/Roaming/XIVLauncher/pluginConfigs/SomethingNeedDoing/stubs" // PluginConfig stubs
  ],

  // Global symbols you want the LSP to treat as defined
  "Lua.diagnostics.globals": [
    "import",
    "yield",
    "Sleep",
    "Wait",
    "Dalamud",
    "Entity",
    "Player",
    "Config",
    "IPC",
    "Instances",
    "TriggerData",
    "ActionType",
    "WeeklyBingoTaskStatus"
  ]
}